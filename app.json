      },
      "package": "com.abigail_br987.prestoopovprestamista"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-font"
    ],
    "extra": {
      "eas": {
        "projectId": "4d468d57-9605-4e5c-9160-5e58a77ce682"
